{
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "10m",
    "max-file": "3",
    "compress": "true"
  },
  "storage-driver": "overlay2",
  "storage-opts": [
    "overlay2.override_kernel_check=true"
  ],
  "max-concurrent-downloads": 3,
  "max-concurrent-uploads": 5,
  "features": {
    "buildkit": true
  },
  "builder": {
    "gc": {
      "enabled": true,
      "defaultKeepStorage": "2GB",
      "policy": [
        {
          "keepStorage": "2GB",
          "filter": [
            "unused-for=24h"
          ]
        },
        {
          "keepStorage": "5GB",
          "all": true
        }
      ]
    }
  },
  "experimental": false
}
